{
  "name": "ui-map",
  "version": "1.0.0",
  "main": "dist/ui-map.common",
  "files": [
    "dist/**/*.*"
  ],
  "scripts": {
    "build": "gms build --target=lib --excludeReact --entry=src/export/index --formats=commonjs",
    "build:doc": "gms build:doc",
    "cache:clean": "rimraf node_modules/.cache",
    "dev:doc": "gms dev:doc",
    "lint": "gms lint",
    "lint:fix": "gms lint --fix",
    "start": "gms dev",
    "test:unit": "gms test:unit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{css,less,sass,scss}": [
      "stylelint --fix",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@gem-mine/immutable": "^1.0.0",
    "@gem-mine/intl": "^2.0.5",
    "@gem-mine/request": "^1.1.4",
    "@sdp.nd/fish": "^3.1.1",
    "classnames": "^2.2.6",
    "core-js": "^3.0.0",
    "lodash": "^4.17.20",
    "mockjs": "^1.0.1-beta3",
    "prop-types": "^15.7.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@gem-mine/babel-preset-app": "^3.2.3-alpha.0",
    "@gem-mine/cli-plugin-doc": "^3.6.0-alpha.0",
    "@gem-mine/cli-plugin-eslint": "^3.3.4-alpha.0",
    "@gem-mine/cli-plugin-jest": "^1.6.8-alpha.0",
    "@gem-mine/cli-plugin-typescript": "^3.0.2-alpha.0",
    "@gem-mine/eslint-config-recommend-ts": "^4.0.2-alpha.0",
    "@gem-mine/script": "^2.20.0-alpha.0",
    "@types/jest": "^26.0.19",
    "@types/node": "14.0.17",
    "@types/prop-types": "^15.7.1",
    "@types/react-loadable": "^5.5.0",
    "@types/webpack-env": "1.15.3",
    "babel-plugin-import": "^1.11.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.5.0",
    "husky": "^2.7.0",
    "jest": "^26.6.3",
    "jest-enzyme": "^7.1.1",
    "lint-staged": "^7.2.2",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-jest": "^26.4.4",
    "typescript": "~4.0.5"
  },
  "component-config": {
    "withRouter": false,
    "withStore": false
  }
}
